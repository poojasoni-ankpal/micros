{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/microservices-learning/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/microservices-learning/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/microservices-learning",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "microservices-learning": {
      "type": "application",
      "root": "apps/microservices-learning",
      "entryFile": "main",
      "sourceRoot": "apps/microservices-learning/src",
      "compilerOptions": {
        "tsConfigPath": "apps/microservices-learning/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "client-service": {
      "type": "application",
      "root": "apps/client-service",
      "entryFile": "main",
      "sourceRoot": "apps/client-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-service/tsconfig.app.json"
      }
    }
  }
}